list_nums=[2,3445,56,6,33,5,3]
l1=sorted(list_nums)
print(l1)


# creation of lists

List=["porsche","ferrari","lamborgini","hennesey"]
print(List.append('BMW'))
print(List)
for x in List:
    if "BMW" in x:
        print('found',x)
        break
    else:
        print('not found',)

        
nums=[1,2,4,5,6]
sum=0
for x in nums:
    sum=sum+x
    print(x,"-->",sum)



# traversing the elemnts in the list

l=[1,2,3,4,5]
i=0
for x in l:
    print("the value is",x,"at index",i)
    i=i+1
